#zad 1 
filmy = ["Skazani na Shawshank","Batman","Nietykalni","Zielona mila","Ojciec chrzestny","Breaking Bad"]
print(filmy)
filmy.reverse()
print(filmy)
filmy.insert(5,"Pianista")
filmy.insert(6,"Incepcja")
filmy.insert(7,"Django")
filmy.insert(8,"Milczenie owiec")
print(filmy)
print(len(filmy))

#zad2
slownik={"tytul0":"Skazani na Shawshank","tytul1":"Batman","tytul2":"Nietykalni","tytul3":"Zielona mila","tytul4":"Ojciec chrzestny","tytul5":"Breaking Bad"}
slownik["tytul6"]="Forrest Gump"
print(slownik)
print(slownik["tytul6"])

